#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/guoweisheng/Desktop/eesa-ui-vue3/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/guoweisheng/Desktop/eesa-ui-vue3/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../terser@5.16.5/node_modules/terser/bin/terser" "$@"
else
  exec node  "$basedir/../../../../../terser@5.16.5/node_modules/terser/bin/terser" "$@"
fi
