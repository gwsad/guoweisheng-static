#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/guoweisheng/领储/vue3-components/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/guoweisheng/领储/vue3-components/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../sass@1.58.3/node_modules/sass/sass.js" "$@"
else
  exec node  "$basedir/../../../../../sass@1.58.3/node_modules/sass/sass.js" "$@"
fi
